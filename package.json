{
    "name": "vanillautils",
    "version": "2.0.0",
    "description": "A zero-dependency collection of 60+ utility functions that can be imported as a module or be added to any website using a minified js file which simplifies developing websites.",
    "main": "vanillaUtils.js",
    "files": [
        "vanillaUtils.js",
        "vanillaUtils.min.js",
        "vanillaUtils.min.js.map",
        "typings/vanillaUtils.d.ts"
    ],
    "types": "typings/vanillaUtils.d.ts",
    "scripts": {
        "test": "jest",
        "build": "terser vanillaUtils.js --compress --mangle --source-map --output vanillaUtils.min.js",
        "minify": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/harshankur/vanillautils.git"
    },
    "keywords": [
        "utils",
        "utilities",
        "helpers",
        "vanilla",
        "javascript",
        "cookies",
        "api",
        "fetch",
        "dom",
        "strings",
        "arrays",
        "objects",
        "async",
        "storage",
        "validation"
    ],
    "author": "Harsh Ankur",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/harshankur/vanillautils/issues"
    },
    "homepage": "https://github.com/harshankur/vanillautils#readme",
    "devDependencies": {
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "jsdoc-to-markdown": "^8.0.0",
        "terser": "^5.44.1",
        "typescript": "^5.0.3"
    },
    "jest": {
        "testEnvironment": "jsdom"
    }
